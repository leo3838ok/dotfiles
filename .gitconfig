[user]
	name = LeoZhan
	email = leozhan@kkbox.com
[core]
	excludesfile = ~/.gitignore
[color]
        ui = auto
[color "branch"]
	current = yellow bold
	local = green bold
	remote = cyan bold
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
	whitespace = red reverse
[color "status"]
	added = green bold
	changed = yellow bold
	untracked = red bold
[alias]
	fp = fetch -p
	#############
        br = branch
	brr = branch -r
	#############
        ci = commit
        cim = commit -m
	#############
        co = checkout
	cotb = checkout --track -b
	#############
        st = status
	#############
	df = diff
	#############
	cp = cherry-pick
	#############
	pl = pull
	plr = pull --rebase
	plo = pull origin
	plro = pull --rebase origin
	#############
	ps = push
	pso = push origin
	psf = push -f
	psfo = push -f origin
	#############
	rb = rebase
	rbi = rebase -i
	#############
	re = reset
	reh = reset --hard
	rehh = reset --hard HEAD
