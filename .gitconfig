[user]
	name = LeoZhan
	email = leozhan@kkbox.com
[core]
	excludesfile = ~/.gitignore
[color]
        ui = auto
[alias]
	fp = fetch -p
	#############
        br = branch
	brr = branch -r
	#############
        ci = commit
        cim = commit -m
	#############
        co = checkout
	cotb = checkout --track -b
	#############
        st = status
	#############
	df = diff
	#############
	cp = cherry-pick
	#############
	pl = pull
	plr = pull --rebase
	plo = pull origin
	plro = pull --rebase origin
	#############
	ps = push
	pso = push origin
	psf = push -f
	psfo = push -f origin
	#############
	rb = rebase
	rbi = rebase -i
	#############
	re = reset
	reh = reset --hard
	rehh = reset --hard HEAD
